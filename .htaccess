RewriteRule ^it/p_(.+)/(.+) prodotto.php?id_prodotto=$1 [L]
RewriteRule ^en/p_(.+)/(.+) prodotto.php?id_prodotto=$1 [L]

RewriteRule ^it/c_(.+)/(.+) categorie.php?id_cat=$1&seo=$2 [L]
RewriteRule ^en/c_(.+)/(.+) categorie.php?id_cat=$1&seo=$2 [L]

RewriteRule ^it/s_(.+)/(.+) scat.php?id_scat=$1&seo=$2 [L]
