RewriteRule  ^$   https://www.sekurbox.com/it/ [L,R=301] 
RewriteRule  ^en/$  index.php?setlng=en [L]
RewriteRule  ^it/$  index.php?setlng=it [L]


RewriteRule ^carts  carts.php [L] 

RewriteRule ^it/elenco-prodotti elenco_prodotti.php?setlng=it [L]
